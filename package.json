    "newtype-ts": "^0.3.2"
  },
  "peerDependencies": {
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.15",
    "@types/ramda": "0.26.12",
    "babel-jest": "24.8.0",
    "dtslint": "0.8.0",
    "fast-check": "1.15.1",
    "husky": "2.7.0",
    "jest": "24.8.0",
    "lint-staged": "8.2.1",
    "normalizr": "3.4.0",
    "prettier": "1.18.2",
    "ramda": "0.26.1",
    "rollup": "1.16.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.1",
    "rollup-plugin-typescript2": "0.21.2",
    "typescript": "3.5.2"
  },
  "scripts": {
    "build": "rollup -c",
    "dtslint": "dtslint test/types",
    "format": "prettier --write **/*.{js,ts,json}",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
